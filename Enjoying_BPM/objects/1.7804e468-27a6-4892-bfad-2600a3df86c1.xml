                <scriptTypeId>2</scriptTypeId>
                <isActive>true</isActive>
                <script>tw.local.comments = new tw.object.listOf.String();

try {
    var processInstance = tw.system.findProcessInstanceByID(tw.local.processInstanceId);
    var tasks = processInstance.tasks;
    
    for (var loop = 0; loop &lt; tasks.length; ++loop) {
        var task = tasks[loop];
        if (task.status == null &amp;&amp; task.subject != null &amp;&amp; task.subject.substr(0, 7) == "Comment") {
            var comment = task.owner + ": " + task.narrative;
            tw.local.comments.insertIntoList(0, comment);
        }
    
    }
}
catch(e) {}</script>
